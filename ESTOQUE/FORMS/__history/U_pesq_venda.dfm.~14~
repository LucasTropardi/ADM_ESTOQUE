  inherited Q_pesq_padrao: TFDQuery
    Active = True
    SQL.Strings = (
      'SELECT'
      '     A.ID_VENDA,'
      '     A.ID_CLIENTE,'
      '     B.NOME,'
      '     A.ID_FORMA_PGTO,'
      '     C.DESCRICAO,'
      '     A.USUARIO,'
      '     A.CADASTRO,'
      '     A.VALOR'
      'FROM VENDA A, CLIENTE B, FORMA_PGTO C'
      'WHERE A.ID_CLIENTE = B.ID_CLIENTE'
      'AND A.ID_FORMA_PGTO = C.ID_FORMA_PGTO'
      'ORDER BY A.ID_VENDA')
    object Q_pesq_padraoID_VENDA: TIntegerField
      FieldName = 'ID_VENDA'
