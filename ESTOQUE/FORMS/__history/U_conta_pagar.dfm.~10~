  object Q_pagar: TFDQuery
    IndexFieldNames = 'ID_COMPRA'
    MasterSource = ds_padrao
    MasterFields = 'ID_COMPRA'
    DetailFields = 'ID_COMPRA'
    Connection = DM.conexao
    SQL.Strings = (
      'SELECT '
      '     ID_SEQUENCIA,'
      '     ID_COMPRA,'
      '     VALOR_PARCELA,'
      '     DT_VENCIMENTO,'
      '     DT_PAGAMENTO,'
      '     ATRASO,'
      '     JUROS,'
      '     VL_JUROS,'
      '     TOTAL_PAGAR,'
      '     STATUS '
      'FROM CONTAS_PAGAR'
      'WHERE ID_COMPRA = :PID_COMPRA'
      'AND STATUS = '#39'EM ABERTO'#39
      'ORDER BY ID_COMPRA')
    Left = 504
    Top = 72
    ParamData = <
      item
        Name = 'PID_COMPRA'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
